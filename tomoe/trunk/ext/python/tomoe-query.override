%%
override tomoe_query_get_readings noargs
static PyObject *
_wrap_tomoe_query_get_readings(PyGObject *self)
{
    GList *readingss;
    PyObject *py_readings;

    readingss = tomoe_query_get_readings (TOMOE_QUERY(self->obj));

    CONVERT_LIST (py_readings, readings);

    g_list_free (readings);

    return py_readings;
}
