override tomoe_query_get_radicals noargs
static PyObject *
_wrap_tomoe_query_get_radicals(PyGObject *self)
{
    GList *radicalss;
    PyObject *py_radicals;

    radicalss = tomoe_query_get_radicals (TOMOE_QUERY(self->obj));

    CONVERT_LIST (py_radicals, radicals);

    g_list_free (radicals);

    return py_radicals;
}
